      <% sets.forEach(user=>{ %>
      <div class="container text-center">
        <div class="row align-items-start">
       
          <div class="col">
            <div class="card w-96 bg-base-100 shadow-xl">
                 
                <figure><img  class = "card-img-top"src=<%= user.img_url %> alt="Shoes" /></figure>
                <div class="card-body">
                  <h2 class="card-title"><%=user.name%></h2>
                  <p>Year: <%= user.year %>    |   Parts: <%= user.num_parts %>  </p>

                  <div class="card-actions justify-end">
                    <a href = "/lego/sets/<%= user.set_num%>">  <button class="btn btn-primary">View Details</button></a>
                

                  </div>
                  
                </div>
              </div>
             
          </div>
           
